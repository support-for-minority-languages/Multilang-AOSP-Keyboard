<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.udmurtlyk.extrainputmethod.latin"
>
    <!-- U+0444: "ф" CYRILLIC SMALL LETTER EF -->
    <Key
        latin:keySpec="&#x0444;" />
    <!-- U+044B: "ы" CYRILLIC SMALL LETTER YERU -->
    <Key
        latin:keySpec="&#x044B;"
        latin:keyHintLabel="!text/morekeys_cyrillic_yy"
        latin:moreKeys="!text/morekeys_cyrillic_yy" />
    <!-- U+0432: "в" CYRILLIC SMALL LETTER VE -->
    <Key
        latin:keySpec="&#x0432;" />
    <!-- U+0430: "а" CYRILLIC SMALL LETTER A -->
    <Key
        latin:keySpec="&#x0430;"
        latin:keyHintLabel="!text/morekeys_cyrillic_a"
        latin:moreKeys="!text/morekeys_cyrillic_a" />
    <!-- U+043F: "п" CYRILLIC SMALL LETTER PE -->
    <Key
        latin:keySpec="&#x043F;" />
    <!-- U+0440: "р" CYRILLIC SMALL LETTER ER -->
    <Key
        latin:keySpec="&#x0440;"
        latin:keyHintLabel="!text/morekeys_cyrillic_er"
        latin:moreKeys="!text/morekeys_cyrillic_er" />
    <!-- U+043E: "о" CYRILLIC SMALL LETTER O -->
    <Key
        latin:keySpec="&#x043E;"
        latin:keyHintLabel="!text/morekeys_cyrillic_o"
        latin:moreKeys="!text/morekeys_cyrillic_o" />
    <!-- U+043B: "л" CYRILLIC SMALL LETTER EL -->
    <Key
        latin:keySpec="&#x043B;"
        latin:keyHintLabel="!text/morekeys_cyrillic_el"
        latin:moreKeys="!text/morekeys_cyrillic_el"  />
    <!-- U+0434: "д" CYRILLIC SMALL LETTER DE -->
    <Key
        latin:keySpec="&#x0434;"/>
    <!-- U+0436: "ж" CYRILLIC SMALL LETTER ZHE -->
    <Key
        latin:keySpec="&#x0436;"
        latin:keyHintLabel="!text/morekeys_cyrillic_dzh"
        latin:moreKeys="!text/morekeys_cyrillic_dzh" />
    <Key
        latin:keySpec="!text/keyspec_east_slavic_row2_11"
        latin:keyHintLabel="!text/morekeys_east_slavic_row2_11"
        latin:moreKeys="!text/morekeys_east_slavic_row2_11" />
</merge>
