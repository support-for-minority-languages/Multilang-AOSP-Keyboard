<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.udmurtlyk.extrainputmethod.latin"
>
    <!-- U+0451: "ё" CYRILLIC SMALL LETTER IO -->
    <Key
        latin:keySpec="&#x0451;"
        latin:keyHintLabel="1"
        latin:additionalMoreKeys="1" />
    <Key
        latin:keySpec="!text/morekeys_cyrillic_dzh"
        latin:keyHintLabel="2"
        latin:additionalMoreKeys="2" />
    <Key
        latin:keySpec="!text/morekeys_cyrillic_o"
        latin:keyHintLabel="3"
        latin:moreKeys="3" />
    <Key
        latin:keySpec="!text/morekeys_cyrillic_tsh"
        latin:keyHintLabel="4"
        latin:additionalMoreKeys="4"/>
    <Key
        latin:keySpec="!text/morekeys_cyrillic_i"
        latin:keyHintLabel="5"
        latin:additionalMoreKeys="5" />
    <Key
        latin:keySpec="&#x04DF;"
        latin:keyHintLabel="6"
        latin:additionalMoreKeys="6"/>
    <!-- U+044C: "ь" CYRILLIC SMALL LETTER SOFT SIGN -->
    <Key
        latin:keySpec="&#x044C;"
        latin:keyHintLabel="7"
        latin:additionalMoreKeys="7"/>
    <!-- U+044A: "ъ" CYRILLIC SMALL LETTER HARD SIGN -->
    <Key
        latin:keySpec="&#x044A;"
        latin:keyHintLabel="8"
        latin:additionalMoreKeys="8" />
    <!-- U+0445: "х" CYRILLIC SMALL LETTER HA -->
    <Key
        latin:keySpec="&#x0445;"
        latin:keyHintLabel="9"
        latin:additionalMoreKeys="9" />
    <!-- U+044D: "э" CYRILLIC SMALL LETTER E -->
    <Key
        latin:keySpec="&#x044D;"
        latin:keyHintLabel="0"
        latin:additionalMoreKeys="0" />
</merge>
